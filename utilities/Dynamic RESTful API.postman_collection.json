					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"pm.test(\"Status code is 200\", function () {\r",
									"    pm.response.to.have.status(200);\r",
									"});\r",
									"pm.test('Has success', function() {\r",
									"    var jsonData = pm.response.json();\r",
									"  pm.expect(jsonData).to.have.property('success');\r",
									"});\r",
									"\r",
									"pm.test(\"Expected response.success to be an array\", function () {\r",
									"    var jsonData = pm.response.json();\r",
									"    pm.expect(jsonData.success).to.be.an('array');\r",
									"    interface = jsonData.success[0];\r",
									"    interface_id = interface.interface_id;\r",
									"    // Set sensor global variables\r",
									"    pm.globals.set(\"interface_id\", interface_id);\r",
									"});"
								],
